@{
    ViewBag.Title = "NewCompany";
    Layout = "~/Areas/Admin/Views/NewLayout.cshtml";
}



<div class="container-fluid">
    <!-- ============================================================== -->
    <!-- Start Page Content -->
    <!-- ============================================================== -->

    <div class="row" style="padding-right:20px;">
        <div class="col-md-12 col-sm-12">
            <div class="card">
                <form class="form-horizontal">
                    <div class="card-body">
                        <div class="row">
                            <div class="col-md-4">
                                <img src="~/Images/deobhog.jpg" />
                            </div>

                            <div class="col-md-3">
                                <b style="font-size:xx-large;color:#132593"> Create a New <br /> Company</b>
                            </div>

                            <div class="col-md-5">
                                <label for="fname" class="col-sm-12 text-left control-label col-form-label" style="font-weight:bold">Enter Name Of Company/Firm/Organization</label>
                                <div class="col-sm-12">
                                    <input type="text" class="form-control" id="fname">
                                </div>

                            </div>
                        </div>

                    </div>
                </form>
            </div>
        </div>
    </div>

    <div class="row" style="margin-top:-20px;">

        <div class="col-md-6">
            <div class="card">
                <form class="form-horizontal">
                    <div class="card-body">

                        <div class="form-group row" style="background-color:#3a89ac">
                            <label for="fname" class="col-sm-12 text-center control-label col-form-label" style="color:white;font-weight:bold">Basic Info</label>
                        </div>

                        <div class="form-group row" style="margin-top:-12px;">
                            <label for="fname" class="col-sm-3 text-left control-label col-form-label">Address Line 1</label>
                            <div class="col-sm-9">
                                <input type="text" class="form-control" id="fname">
                            </div>
                        </div>
                        <div class="form-group row" style="margin-top:-12px;">
                            <label for="lname" class="col-sm-3 text-left control-label col-form-label">Address Line 2</label>
                            <div class="col-sm-9">
                                <input type="text" class="form-control" id="lname">
                            </div>
                        </div>

                        <div class="form-group row" style="margin-top:-12px;">
                            <label for="lname" class="col-sm-3 text-left control-label col-form-label">City</label>
                            <div class="col-sm-3">
                                <input type="password" class="form-control" id="lname">
                            </div>
                            <label for="email1" class="col-sm-3 text-left control-label col-form-label">Pincode</label>
                            <div class="col-sm-3">
                                <input type="text" class="form-control" id="email1">
                            </div>

                        </div>

                        <div class="form-group row" style="background-color:#3a89ac; margin-top:-11px;">
                            <label for="email1" class="col-sm-12 text-center control-label col-form-label" style="color:white;font-weight:bold">Location Info</label>
                        </div>


                        <div class="form-group row" style="margin-top:-12px;">
                            <label for="cono1" class="col-sm-3 text-left control-label col-form-label">Countyr {ISD}</label>
                            <div class="col-sm-9">
                                <input type="text" class="form-control" id="cono1" placeholder="India (+91)">
                            </div>
                        </div>

                        <div class="form-group row" style="margin-top:-12px;">
                            <label for="lname" class="col-sm-3 text-left control-label col-form-label">State</label>
                            <div class="col-sm-3">
                                <input type="password" class="form-control" id="lname">
                            </div>
                            <label for="email1" class="col-sm-3 text-left control-label col-form-label">State Code</label>
                            <div class="col-sm-3">
                                <input type="text" class="form-control" id="email1">
                            </div>

                        </div>

                        <div class="form-group row" style="margin-top:-12px;">
                            <label for="lname" class="col-sm-3 text-left control-label col-form-label">Distt.</label>
                            <div class="col-sm-3">
                                <input type="password" class="form-control" id="lname">
                            </div>
                            <label for="email1" class="col-sm-3 text-left control-label col-form-label">Area</label>
                            <div class="col-sm-3">
                                <input type="text" class="form-control" id="email1">
                            </div>

                        </div>

                        <div class="form-group row" style="background-color:#3a89ac;margin-top:-12px;">
                            <label for="cono1" class="col-sm-12 text-center control-label col-form-label" style="color:white;font-weight:bold">Contact Info</label>
                        </div>

                        <div class="form-group row" style="margin-top:-12px;">
                            <label for="lname" class="col-sm-3 text-left control-label col-form-label">Mobile</label>
                            <div class="col-sm-3">
                                <input type="password" class="form-control" id="lname">
                            </div>
                            <label for="email1" class="col-sm-3 text-left control-label col-form-label">Phone</label>
                            <div class="col-sm-3">
                                <input type="text" class="form-control" id="email1">
                            </div>

                        </div>

                        <div class="form-group row" style="margin-top:-12px;">
                            <label for="lname" class="col-sm-3 text-left control-label col-form-label">Email</label>
                            <div class="col-sm-3">
                                <input type="password" class="form-control" id="lname">
                            </div>
                            <label for="email1" class="col-sm-3 text-left control-label col-form-label">Website</label>
                            <div class="col-sm-3">
                                <input type="text" class="form-control" id="email1">
                            </div>

                        </div>



                        <div class="form-group row" style="background-color:#3a89ac;margin-top:-12px;">
                            <label for="cono1" class="col-sm-12 text-center control-label col-form-label" style="color:white;font-weight:bold">Tax Registration Detail</label>
                        </div>

                        <div class="form-group row" style="margin-top:-12px;">
                            <label for="lname" class="col-sm-3 text-left control-label col-form-label">GST No.</label>
                            <div class="col-sm-3">
                                <input type="password" class="form-control" id="lname">
                            </div>
                            <label for="email1" class="col-sm-3 text-left control-label col-form-label">VAT No.</label>
                            <div class="col-sm-3">
                                <input type="text" class="form-control" id="email1">
                            </div>

                        </div>

                        <div class="form-group row" style="margin-top:-12px;">
                            <label for="lname" class="col-sm-3 text-left control-label col-form-label">PAN No.</label>
                            <div class="col-sm-3">
                                <input type="password" class="form-control" id="lname">
                            </div>
                            <label for="email1" class="col-sm-3 text-left control-label col-form-label">CST No.</label>
                            <div class="col-sm-3">
                                <input type="text" class="form-control" id="email1">
                            </div>

                        </div>

                        <div class="form-group row" style="margin-top:-12px;">
                            <label for="fname" class="col-sm-3 text-left control-label col-form-label">Deals In</label>
                            <div class="col-sm-9">
                                <input type="text" class="form-control" id="fname">
                            </div>
                        </div>

                    </div>

                </form>
            </div>



        </div>
        <div class="col-md-6" style="margin-left:-20px;">
            <div class="card">
                <div class="card-body">
                    <div class="form-group row" style="background-color:#3a89ac">
                        <label for="fname" class="col-sm-12 text-center control-label col-form-label" style="color:white;font-weight:bold">Financial Year Date Range</label>
                    </div>

                    <div class="form-group row">
                        <label for="fname" class="col-sm-6 text-left control-label col-form-label">Financial Year Starting Date</label>
                        <div class="col-sm-6">
                            <input type="date" class="form-control" id="fname">
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="lname" class="col-sm-6 text-left control-label col-form-label">Financial Year Will Close On</label>
                        <div class="col-sm-6">
                            <input type="date" class="form-control" id="lname">
                        </div>
                    </div>


                    <div class="form-group row" style="background-color:#3a89ac">
                        <label for="email1" class="col-sm-12 text-center control-label col-form-label" style="color:white;font-weight:bold">Guidance and Training Engineer Detail</label>
                    </div>


                    <div class="form-group row">
                        <label for="lname" class="col-sm-3 text-left control-label col-form-label">Engineer Name</label>
                        <div class="col-sm-3">
                            <input type="password" class="form-control" id="lname">
                        </div>
                        <label for="email1" class="col-sm-3 text-left control-label col-form-label">Engineer Phone</label>
                        <div class="col-sm-3">
                            <input type="text" class="form-control" id="email1">
                        </div>

                    </div>

                    <div class="form-group row">
                        <label for="lname" class="col-sm-4 text-left control-label col-form-label">Another partner Code</label>
                        <div class="col-sm-8">
                            <input type="password" class="form-control" id="lname">
                        </div>


                    </div>

                    <div class="form-group row" style="background-color:#3a89ac;margin-top:10px;">
                        <label for="cono1" class="col-sm-12 text-center control-label col-form-label" style="color:white;font-weight:bold">Copy Master Data From An Existing Company</label>
                    </div>

                    <div class="form-group row">
                        <div class="col-sm-2">
                            <input type="checkbox" class="form-control" id="lname">
                        </div>
                        <label for="lname" class="col-sm-9 text-left control-label col-form-label" style="font-weight:bold">Copy Master From An Existing Company</label>

                    </div>





                    <div class="form-group row" style="background-color: #216ca5;margin-top:34px;">
                        <label for="cono1" class="col-sm-12 text-center control-label col-form-label" style="color:white;font-weight:bold;">Make a Super User For This Company</label>
                    </div>

                    <div class="form-group row">
                        <label for="lname" class="col-sm-3 text-left control-label col-form-label">User Name :</label>
                        <div class="col-sm-3">
                            <input type="password" class="form-control" id="lname">
                        </div>
                        <label for="email1" class="col-sm-3 text-left control-label col-form-label">Password :</label>
                        <div class="col-sm-3">
                            <input type="text" class="form-control" id="email1">
                        </div>

                    </div>

                    <div class="form-group row">
                        <button type="button" class="btn btn-primary col-md-5" style="width: 200px; background-color: #3a89ac">Create Company</button>
                        <div class="col-md-2"></div>
                        <button type="button" class="btn btn-primary col-md-5" style="width:200px;">No Thanks, Close It</button>
                    </div>
                  

                </div>
            </div>



        </div>
    </div>
    <!-- editor -->


</div>




